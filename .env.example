DATABASE_URL=postgres://cm_user:cm_pass@postgres:5432/cm_db

# Frontend
NEXT_PUBLIC_API_URL=http://backend:3001

# PgAdmin (optional)
PGADMIN_DEFAULT_EMAIL=admin@example.com
